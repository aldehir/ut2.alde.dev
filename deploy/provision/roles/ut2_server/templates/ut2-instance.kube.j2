[Service]
TimeoutStartSec=900

[Kube]
Yaml={{ ut2_server_instance_name }}.yml
{% if ut2_server_network is not none %}
Network={{ ut2_server_network }}
{% endif %}
PublishPort={{ ut2_server_game_port }}:7777/udp
PublishPort={{ ut2_server_query_port }}:7778/udp
PublishPort={{ ut2_server_gamespy_port }}:7787/udp
PublishPort={{ ut2_server_web_admin_port }}:80/tcp

[Install]
WantedBy=multi-user.target
